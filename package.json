    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "netlify-cli": "^17.10.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
